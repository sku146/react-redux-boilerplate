{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "Simple React Redux Sample Project",
  "main": "index.js",
  "scripts": {
    "start": "engine --server",
    "release": "engine --build production",
    "lint": "engine --lint",
    "lint:report": "engine -r",
    "test:watch": "engine --test watch",
    "test:watchAll": "engine --test watchAll",
    "test:unit": "engine --test",
    "flow": "flow",
    "doc": "engine --doc",
    "check": "npm-check -u",
    "test": "engine -l -r -t",
    "mock:start": "engine --mockServer",
    "mock:watch": "engine --mockServer watch",
    "mock:db-watch": "json-server --watch configs/engine/mock-server/db.json",
    "scout": "engine --build production -c appJourney"
  },
  "keywords": [
    "React",
    "Redux",
    "Webpack",
    "Boilerplate",
    "Application"
  ],
  "precommit": [
    "test"
  ],
  "author": {
    "name": "SAPIENT",
    "email": "skumar146@sapient.com"
  },
  "license": "UNLICENSED",
  "engines": {
    "node": ">= 6",
    "npm": ">= 3"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "guid": "0.0.12",
    "history": "^4.7.2",
    "lodash": "^4.17.4",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-form": "^7.1.2",
    "semantic-ui-react": "^0.76.0",
