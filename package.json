    "redux-saga": "^0.16.0",
    "superagent": "^3.8.1",
    "redux-logger": "^3.0.6",
    "flow-babel-webpack-plugin": "^1.1.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0"
  },
  "devDependencies": {
    "app-build-engine": "^2.0.0",
    "autoprefixer": "^7.1.6",
